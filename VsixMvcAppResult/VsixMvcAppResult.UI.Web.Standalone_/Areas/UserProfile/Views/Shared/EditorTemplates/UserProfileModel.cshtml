@model VsixMvcAppResult.Models.Profile.UserProfileModel
@using VsixMvcAppResult.UI.Web.Common.Mvc.Html;
@using VsixMvcAppResult.UI.Web.Areas.UserProfile;
@using VsixMvcAppResult.Models.Enumerations;
@using VsixMvcAppResult.Models.Globalization;
@using VsixMvcAppResult.UI.Web.Common.Mvc.Html;


@{
    using (MvcWidgetBox divBox = Html.BeginWidgetBox())
    {
        using (MvcWidgetBox divBoxHeader = Html.BeginWidgetBoxHeader())
        { 
            @VsixMvcAppResult.Resources.Account.AccountResources.ProfileEdit
        }
        using (MvcWidgetBox divBoxContent = Html.BeginWidgetBoxContent())
        {
        <text>
            @Html.FormItem(model => model.FirstName)
            @Html.FormItem(model => model.LastName)
            @Html.FormItem(model => model.BirthDate)
            @Html.FormItem(model => model.Gender)
        </text>
        }
    }
}

