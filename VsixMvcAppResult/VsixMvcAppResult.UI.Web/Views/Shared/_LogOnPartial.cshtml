@using VsixMvcAppResult.UI.Web.Common.Mvc.Html;
@model VsixMvcAppResult.UI.Web.Models.baseViewModel

@{
    if (this.Model.BaseViewModelInfo.IsUserIntendedController)
    {
        if (this.Model.BaseViewModelInfo.UserIsLoggedIn)
        {
            @Html.LogOffButton()
        }
        else
        {
            @Html.LogOnButton()
        }
    }
}

