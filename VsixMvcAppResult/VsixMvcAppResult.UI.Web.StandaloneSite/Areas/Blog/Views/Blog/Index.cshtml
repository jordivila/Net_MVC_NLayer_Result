@using VsixMvcAppResult.UI.Web.Common.Mvc.Html;
@using VsixMvcAppResult.UI.Web.Areas.Blog;
@using System.ServiceModel.Syndication;
@model VsixMvcAppResult.UI.Web.Areas.Blog.Models.BlogModel

@{Layout = "~/Views/Shared/_LayOut.cshtml";}

@section ScriptsToAdd
{

}

@section InlineStyles{
    
}

<p>
@VsixMvcAppResult.Resources.Blog.BlogTexts.BrowseBy <a href="@Url.Category(string.Empty)">@VsixMvcAppResult.Resources.Blog.BlogTexts.Category</a>
</p>
<br />
@foreach (var item in this.Model.FeedItems.Data)
{
        @Html.DisplayFor(x => item.Item)
}
