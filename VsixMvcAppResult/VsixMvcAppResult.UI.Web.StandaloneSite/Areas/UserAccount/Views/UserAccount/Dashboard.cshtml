@using VsixMvcAppResult.UI.Web.Common.Mvc.Html;
@using VsixMvcAppResult.UI.Web.Areas.UserAdministration;
@using VsixMvcAppResult.UI.Web.Areas.Error;
@using VsixMvcAppResult.Models.Profile;
@using VsixMvcAppResult.UI.Web.Areas.UserAccount.Models;
@model DashBoardViewModel

@{Layout = "~/Views/Shared/_LayOut.cshtml";}

@section ScriptsToAdd
{
        <script type="text/javascript">
            (function ($) {
                jQuery(document).ready(function () {
                    VsixMvcAppResult.Widgets.Page.onInit(function () {
                        jQuery('#dashBoardMenu').navMenu();
                    });
                });
            })(jQuery);
        </script>    
}

@section InlineStyles{
    <style type="text/css">
        #dashBoardMenu {border:0px solid transparent;}
    </style>
}


@{
    using (MvcForm form = Html.BeginWidgetForm())
    { 
        using (MvcWidgetBox boxSite = Html.BeginWidgetBox(new { @class = "ui-dashboard-box" }))    
        {
            using (MvcWidgetBox divBoxHeader = Html.BeginWidgetBoxHeader())
            { 
                <text>
                    @this.Model.BaseViewModelInfo.UserFormsIdentityName
                </text>
            }
            using (MvcWidgetBox divBoxContent = Html.BeginWidgetBoxContent())
            {
                <text>
                    @this.Model.DashboardMenu.Render(new { id ="dashBoardMenu" })
                </text>
            }
        }
    }
}






